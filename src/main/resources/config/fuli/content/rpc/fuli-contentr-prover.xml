<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <!-- catalog services begin -->
    <bean id="hsfCenterContentCatalogApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.ICatalogApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentCatalogApiService"/>
    </bean>
    <bean id="hsfCenterContentCatalogQueryApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.query.ICatalogQueryApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentCatalogQueryApiService"/>
    </bean>
    <!-- catalog services end -->

    <!-- article services begin -->
    <bean id="hsfCenterContentArticleApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.IArticleApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentArticleApiService"/>
    </bean>
    <bean id="hsfCenterContentArticleStatApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.IArticleStatApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentArticleStatApiService"/>
    </bean>
    <bean id="hsfCenterContentArticleQueryApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.query.IArticleQueryApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentArticleQueryApiService"/>
    </bean>
    <!-- article services end -->

    <!-- comment services begin -->
    <bean id="hsfCenterContentCommentApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.ICommentApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentCommentApiService"/>
    </bean>
    <bean id="hsfCenterContentCommentQueryApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.query.ICommentQueryApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentCommentQueryApiService"/>
    </bean>
    <!-- comment services end -->

    <!-- member action services begin -->
    <bean id="hsfCenterContentMemberActionApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.IMemberActionApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentMemberActionApiService"/>
    </bean>
    <bean id="hsfCenterContentMemberActionQueryApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.query.IMemberActionQueryApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentMemberActionQueryApiService"/>
    </bean>
    <!-- member action services end -->

    <!-- tag services begin -->
    <bean id="hsfCenterContentTagApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.ITagApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentTagApiService"/>
    </bean>
    <bean id="hsfCenterContentTagQueryApiService" class="com.taobao.hsf.app.spring.util.HSFSpringProviderBean" init-method="init">
        <property name="serviceInterface" value="com.dtyunxi.newretail.center.content.api.query.ITagQueryApiService"/>
        <property name="serviceVersion" value="${dtyunxi.service.version}"/>
        <property name="serviceGroup" value="${dtyunxi.service.group}"/>
        <property name="target" ref="centerContentTagQueryApiService"/>
    </bean>
    <!-- tag services end -->

</beans>